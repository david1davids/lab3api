<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <ListView fx:id="list" layoutX="14.0" layoutY="14.0" prefHeight="365.0" prefWidth="459.0" />
            <Button fx:id="add" layoutX="494.0" layoutY="24.0" mnemonicParsing="false" onAction="#onClickAdd" prefHeight="25.0" prefWidth="92.0" text="Add User" />
            <Button fx:id="delete" layoutX="494.0" layoutY="97.0" mnemonicParsing="false" onAction="#onClickDelete" prefHeight="25.0" prefWidth="92.0" text="Delete User" />
            <Button fx:id="edit" layoutX="494.0" layoutY="60.0" mnemonicParsing="false" onAction="#onClickEdit" prefHeight="25.0" prefWidth="92.0" text="Edit User" />
         </children>
      </Pane>
   </children>
</AnchorPane>
