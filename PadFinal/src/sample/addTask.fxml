<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddTaskController">
    <children>
        <Pane prefHeight="300.0" prefWidth="400.0">
            <children>
                <TextField fx:id="taskname" layoutX="10.0" layoutY="55.0" prefHeight="25.0" prefWidth="381.0" promptText="taskname" />
                <Button fx:id="add" layoutX="317.0" layoutY="253.0" mnemonicParsing="false" onAction="#onClickAdd" prefHeight="33.0" prefWidth="69.0" style="-fx-background-color: #ffffff; -fx-border-color: #dddddd; -fx-cursor: hand;" text="Add Task" />
                <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onClickBack" prefHeight="33.0" prefWidth="69.0" style="-fx-background-color: #ffffff; -fx-border-color: #dddddd; -fx-cursor: hand;" text="Back" />
            <CheckBox fx:id="completed" layoutX="12.0" layoutY="261.0" mnemonicParsing="false" text="Completed" />
            <ListView fx:id="list" layoutX="10.0" layoutY="106.0" prefHeight="118.0" prefWidth="381.0" />
            <Label layoutX="14.0" layoutY="89.0" text="Assign-to" />
            <Label fx:id="labelAssign" layoutX="10.0" layoutY="228.0" prefHeight="25.0" prefWidth="381.0" />
            </children>
        </Pane>
    </children>
</AnchorPane>
